<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="app_guiads.Views.ComponentesPrimeiro"
             BackgroundColor="#f3f2f0"
             Title="Disciplinas do Primeiro Ano">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout VerticalOptions="CenterAndExpand" Padding="10">

                
                <Button Text="Técnicas de Programação e Algoritmos" Clicked="Button_Clicked_1" Style="{ StaticResource botaopreto }" />
                <Button Text="Banco de Dados I" Clicked="Button_Clicked_2" Style="{ StaticResource botaopreto }" />
                <Button Text="Análise de Sistemas" Clicked="Button_Clicked_3" Style="{ StaticResource botaopreto }" />
                <Button Text="Design Digital" Clicked="Button_Clicked_4" Style="{ StaticResource botaopreto }" />
                <Button Text="Programação Web I" Clicked="Button_Clicked_5" Style="{ StaticResource botaopreto }" />
                <Button Text="Fundamentos da Informática" Clicked="Button_Clicked_6" Style="{ StaticResource botaopreto }" />
                

            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>